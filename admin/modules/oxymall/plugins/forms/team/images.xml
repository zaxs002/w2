<form>

	<forms>
		<search>members/search.xml</search>
		<add>members/add.xml</add>
		<edit>members/edit.xml</edit>
		<list>members/list.xml</list>
	</forms>

	<table>plugin:team_items</table>
	<table_uid>item_id</table_uid>


	<URIdata>
		<action>action</action>

		<add>add</add>
		<details>details</details>
		<delete>delete</delete>
		<edit>edit</edit>
		<list>details</list>
		<search>search</search>

		<mup>moveup</mup>
		<mdw>movedown</mdw>

		<store>store</store>
	</URIdata>


	<URIlinks>
		<add>{SELF.LINK.ADD}&item_cat={ITEM_CAT}&module_id={MODULE_ID}&returnurl={SELF.LOCATION}</add>
		<edit>{SELF.LINKUID.EDIT}&module_id={MODULE_ID}&item_cat={ORIGINAL_ITEM_CAT}&returnurl={SELF.LOCATION}</edit>
		<details>{SELF.LINKUID.DETAILS}&module_id={MODULE_ID}&returnurl={SELF.LOCATION}</details>
		<delete>javascript:if(confirm('Are you sure you want to delete this record ?')) window.location='{SELF.LINKUID.DELETE}&module_id={MODULE_ID}&returnURL={SELF.LOCATION}'</delete>
		<store>{SELF.LINK}{SELF.ACTION.STORE}&module_id={MODULE_ID}</store>

		<move_down>{SELF.LINKUID.MDW}&module_id={MODULE_ID}&item_order={ITEM_ORDER}&&returnurl={SELF.LOCATION}</move_down>
		<move_up>{SELF.LINKUID.MUP}&module_id={MODULE_ID}&item_order={ITEM_ORDER}&returnurl={SELF.LOCATION}</move_up>
	</URIlinks>



</form>